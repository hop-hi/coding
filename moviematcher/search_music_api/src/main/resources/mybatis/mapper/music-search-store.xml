<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mh.music.moviematcher.repository.MusicSearchRepository">
    <insert id="save" parameterType="com.mh.music.moviematcher.vo.MusicSearchVO">
        insert into music_search_result
        (music_name, music_url)
        values(#{musicName} , #{musicURL})
    </insert>
</mapper>